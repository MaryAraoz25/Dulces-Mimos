	<queryString>
		<![CDATA[SELECT 
    '001-002-' || LPAD(v.idventas::text, 6, '0') AS idventa_format,
    v.ventas_fecha,
    v.hora,
    cl.cli_nombre,
    cl.cli_apellido,
    cl.cli_ruc,
    v.venta_timbrado,
    v.venta_inicio AS fecha_inicio,
    v.venta_fin AS fecha_fin,
    CASE 
        WHEN v.condicion_venta = 'Contado' THEN 'X'
        ELSE ''
    END AS colum_contado,
    CASE 
        WHEN v.condicion_venta = 'Credito' THEN 'X'
        ELSE ''
    END AS colum_credito
FROM 
    public.ventas v
JOIN 
    public.clientes cl ON v.clientes_id = cl.idclientes
WHERE 
    v.idventas = $P{cab}
GROUP BY 
    idventa_format,
    v.ventas_fecha,
    v.hora,
    cl.cli_nombre,
    cl.cli_apellido,
    cl.cli_ruc,
    v.venta_timbrado,
    v.venta_inicio,
    v.venta_fin,
    v.condicion_venta;]]>
	</queryString>
