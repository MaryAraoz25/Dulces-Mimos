	<parameter name="fecha_desde_pagos" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fecha_hasta_pagos" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT 
    p.id,
    p.fecha_pago,
    p.estado_pago,
    CONCAT(e.emple_nombre, ' ', e.emple_apellido) AS empleado_nombre,
    pr.prov_nombre,
    m.metpag_nombre,
    p.num_dias,
    p.total_pagar
FROM 
    pagos p
JOIN 
    empleados e ON p.empleado_id = e.idempleados
JOIN 
    proveedores pr ON p.proveedor_id = pr.idproveedores
JOIN 
    metodos_pago m ON p.met_pago = m.idmetodos_pago
WHERE 
    p.fecha_pago BETWEEN $P{fecha_desde_pagos} AND $P{fecha_hasta_pagos};]]>
	</queryString>
