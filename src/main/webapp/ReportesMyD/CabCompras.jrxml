	<queryString>
		<![CDATA[SELECT 
    '001-002-' || LPAD(c.idcompras::text, 6, '0') AS idcompra_format,
    c.idcompras,
    c.compras_fecha,
    c.compras_hora,
    c.total_compra,
    c.iva_5,
    c.iva_10,
    c.num_dias,
    p.prov_nombre,
    p.prov_ruc,
    p.prov_direccion,
    p.prov_timbrado,
    p.prov_inicio AS fecha_inicio,
    p.prov_fin AS fecha_fin,
    CASE 
        WHEN c.condicion_compra = 'Contado' THEN 'X'
        ELSE ''
    END AS colum_contado,
    CASE 
        WHEN c.condicion_compra = 'Credito' THEN 'X'
        ELSE ''
    END AS colum_credito
FROM 
    public.compras c
JOIN 
    public.proveedores p ON c.proveedores_id = p.idproveedores
JOIN 
    public.detalle_compras d ON c.idcompras = d.compra_id
WHERE 
    c.idcompras = $P{cab}
GROUP BY 
    c.idcompras, c.compras_fecha, c.compras_hora, c.total_compra, c.iva_5, c.iva_10, c.num_dias,
    p.prov_nombre, p.prov_ruc, p.prov_direccion, p.prov_timbrado, p.prov_inicio, p.prov_fin,
    c.condicion_compra;]]>
	</queryString>
