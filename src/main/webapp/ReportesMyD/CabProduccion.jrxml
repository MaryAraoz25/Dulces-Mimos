	<queryString>
		<![CDATA[SELECT 
    p.idproduccion,
    p.fecha_elaboracion,
    p.fecha_vencimiento,
    p.cantidad,
    r.nombre_receta
FROM 
    produccion p
JOIN 
    recetas r ON p.receta_id = r.idrecetas_productos
WHERE 
    p.idproduccion = $P{cab}
GROUP BY 
    p.idproduccion,
    p.fecha_elaboracion,
    p.fecha_vencimiento,
    p.cantidad,
    r.nombre_receta;]]>
	</queryString>
