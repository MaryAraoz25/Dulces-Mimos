	<queryString>
		<![CDATA[SELECT 
    p.idproductos, 
    p.pro_nombre, 
    p.pro_precio, 
    p.stock_actual, 
    p.stock_minimo, 
    p.impuesto, 
    c.categ_nombre AS categoria, 
    u.descripcion AS unidad_de_medida
FROM 
    productos p
JOIN 
    categ_productos c 
ON 
    p.categ_productos = c.idcateg_productos
JOIN 
    unidad_medida u 
ON 
    p.unidad_de_medida_id = u.idunidad_medida
where idproductos = $P{detalle};]]>
	</queryString>
