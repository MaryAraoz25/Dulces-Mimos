	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="cab" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT 
    pr.pro_nombre,
    dp.detpedidos_cantidad,
    dp.detpedidos_preciounitario
FROM 
    public.detalle_pedidos dp
JOIN 
    public.productos pr ON dp.productos_id = pr.idproductos
WHERE 
    dp.pedidos_id = $P{cab};]]>
	</queryString>
	<field name="pro_nombre" class="java.lang.String"/>
	<field name="detpedidos_cantidad" class="java.lang.Integer"/>
	<field name="detpedidos_preciounitario" class="java.lang.Integer"/>
	<variable name="subtotal" class="java.lang.Integer">
		<variableExpression><![CDATA[$F{detpedidos_cantidad}*$F{detpedidos_preciounitario}]]></variableExpression>
	</variable>
	<variable name="resultado" class="java.lang.Integer" calculation="Sum">
		<variableExpression><![CDATA[$V{subtotal}]]></variableExpression>
	</variable>
