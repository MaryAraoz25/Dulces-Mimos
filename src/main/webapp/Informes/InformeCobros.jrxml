	<parameter name="fecha_desde_cobros" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fecha_hasta_cobros" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT 
    c.idcobro,
    c.fecha_cobro,
    c.estado_cobro,
    CONCAT(e.emple_nombre, ' ', e.emple_apellido) AS empleado_nombre,
    CONCAT(cl.cli_nombre, ' ', cl.cli_apellido) AS cliente_nombre,
    m.metpag_nombre,
    c.num_dias,
    c.total_cobro
FROM 
    cobros c
JOIN 
    empleados e ON c.empleado_id = e.idempleados
JOIN 
    clientes cl ON c.cliente_id = cl.idclientes
JOIN 
    metodos_pago m ON c.met_pag = m.idmetodos_pago
WHERE 
    c.fecha_cobro BETWEEN $P{fecha_desde_cobros} AND $P{fecha_hasta_cobros};]]>
	</queryString>
