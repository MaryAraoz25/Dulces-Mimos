	<parameter name="fecha_desde_pedidos" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fecha_hasta_pedidos" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="cli_informe" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT 
    p.idpedidos AS id,
    p.fecha_pedido AS fecha,
    CONCAT(c.cli_nombre, ' ', c.cli_apellido) AS nombre_cliente,
    p.estado_pedidos,
    p.estado_stock,
    p.estado_entrega
FROM 
    public.pedidos p
JOIN 
    public.clientes c ON p.cliente_id = c.idclientes
LEFT JOIN 
    public.empleados e ON p.empleados_id = e.idempleados
WHERE 
    p.fecha_pedido BETWEEN $P{fecha_desde_pedidos} AND $P{fecha_hasta_pedidos}
    AND ($P{cli_informe} IS NULL OR p.cliente_id = $P{cli_informe})
AND p.estado_pedidos = 'Finalizado';]]>
	</queryString>
	<field name="id" class="java.lang.Integer"/>
	<field name="fecha" class="java.sql.Date"/>
	<field name="nombre_cliente" class="java.lang.String"/>
	<field name="estado_pedidos" class="java.lang.String"/>
	<field name="estado_stock" class="java.lang.String"/>
	<field name="estado_entrega" class="java.lang.String"/>
