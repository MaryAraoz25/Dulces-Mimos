	<queryString>
		<![CDATA[SELECT 
    dp.cantidad,
    i.ingre_nombre,
    um.descripcion AS unidad_medida
FROM 
    detalle_produccion dp
JOIN 
    ingredientes i ON dp.ingredientes_id = i.idingredientes
JOIN 
    unidad_medida um ON i.unidad_de_medida_id = um.idunidad_medida
WHERE 
    dp.produccion_id = $P{cab};]]>
	</queryString>
