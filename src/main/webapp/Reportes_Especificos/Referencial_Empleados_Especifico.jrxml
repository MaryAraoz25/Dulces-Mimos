	<queryString>
		<![CDATA[SELECT 
    e.idempleados,
    e.emple_nombre,
    e.emple_apellido,
    e.emple_ci,
    e.emple_direccion,
    e.emple_estado,
    e.emple_telefono,
    e.emple_correo,
    u.usu_nombre AS nombre_usuario
FROM 
    public.empleados e
JOIN 
    public.usuarios u
ON 
    e.usuarios_id = u.idusuarios where idempleados= $P{detalle};]]>
	</queryString>
