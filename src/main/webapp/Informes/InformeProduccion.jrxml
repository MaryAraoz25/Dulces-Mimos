	<parameter name="fecha_desde_produccion" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fecha_hasta_produccion" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="produc_informe" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT 
    p.idproduccion,
    p.fecha_elaboracion,
    p.cantidad,
    p.fecha_vencimiento,
    p.estado_produccion,
    e.emple_nombre,
    r.nombre_receta
FROM 
    public.produccion p
JOIN 
    public.recetas r ON p.receta_id = r.idrecetas_productos
JOIN 
    public.empleados e ON p.empleados_id = e.idempleados
WHERE 
    p.fecha_elaboracion BETWEEN $P{fecha_desde_produccion} AND $P{fecha_hasta_produccion}
    AND (
        $P{produc_informe} IS NULL 
        OR p.receta_id = $P{produc_informe}  
    )
and p.estado_produccion = 'Finalizado';]]>
	</queryString>
