	<parameter name="empleado_id" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fecha_desde" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fecha_hasta" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT 
    r.idrecetas_productos AS id, 
    r.nombre_receta, 
    CONCAT(e.emple_nombre, ' ', e.emple_apellido) AS empleado, 
    r.recetaspro_estado AS estado
FROM 
    recetas r
INNER JOIN 
    empleados e ON r.empleados_id = e.idempleados
WHERE 
    r.recetaspro_estado = 'Finalizado'
    AND r.fecha_receta BETWEEN $P{fecha_desde} AND $P{fecha_hasta} 
    AND ($P{empleado_id} IS NULL OR r.empleados_id = $P{empleado_id})
AND r.recetaspro_estado = 'Finalizado';]]>
	</queryString>
	<field name="id" class="java.lang.Integer"/>
	<field name="nombre_receta" class="java.lang.String"/>
	<field name="empleado" class="java.lang.String"/>
	<field name="estado" class="java.lang.String"/>
