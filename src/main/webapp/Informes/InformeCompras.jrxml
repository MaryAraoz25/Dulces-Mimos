	<parameter name="fecha_desde" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fecha_hasta" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="prov_informe" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT 
    c.idcompras,
    c.compras_fecha,
    c.compras_estado,
    e.emple_nombre,  
    p.prov_nombre,
    c.total_compra
FROM 
    compras c
JOIN 
    proveedores p ON c.proveedores_id = p.idproveedores
JOIN 
    empleados e ON c.empleados_id = e.idempleados  
WHERE 
    c.compras_fecha BETWEEN $P{fecha_desde} AND $P{fecha_hasta}
    AND ($P{prov_informe} IS NULL OR c.proveedores_id = $P{prov_informe})
AND c.compras_estado = 'Finalizado';]]>
	</queryString>
	<field name="idcompras" class="java.lang.Integer"/>
	<field name="compras_fecha" class="java.sql.Date"/>
	<field name="compras_estado" class="java.lang.String"/>
