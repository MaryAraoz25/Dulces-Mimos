	<parameter name="fecha_desde_ventas" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fecha_hasta_ventas" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="informe_cliente" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT 
    v.idventas,
    v.ventas_fecha,
    v.ventas_estado,
    CONCAT(e.emple_nombre, ' ', e.emple_apellido) AS empleado_completo,
    CONCAT(c.cli_nombre, ' ', c.cli_apellido) AS cliente_completo,
    v.total_venta
FROM 
    public.ventas v
JOIN 
    public.empleados e ON v.empleados_id = e.idempleados
JOIN 
    public.clientes c ON v.clientes_id = c.idclientes
WHERE 
    v.ventas_fecha BETWEEN $P{fecha_desde_ventas} AND $P{fecha_hasta_ventas}
    AND ($P{informe_cliente} IS NULL OR c.idclientes = $P{informe_cliente})
    AND v.ventas_estado = 'Finalizado';]]>
	</queryString>
	<field name="idventas" class="java.lang.Integer"/>
	<field name="ventas_fecha" class="java.sql.Date"/>
	<field name="ventas_estado" class="java.lang.String"/>
	<field name="empleado_completo" class="java.lang.String"/>
	<field name="cliente_completo" class="java.lang.String"/>
	<field name="total_venta" class="java.lang.Integer"/>
	<variable name="total_ventas" class="java.lang.Integer" calculation="Sum">
		<variableExpression><![CDATA[$F{total_venta}]]></variableExpression>
	</variable>
